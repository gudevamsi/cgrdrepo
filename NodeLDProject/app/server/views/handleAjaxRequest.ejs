						var viewprojroleform=document.getElementById('viewprojroleform');
						//alert('<%=projroleid%> -->'+viewprojroleform);
						viewprojroleform.submit();
					}
					submitForm();
				</script>
			</html>
		<% } %>
	<% } %>
	<% if(from == 'addcertification') {%>
		<% if(typeof certificationid != 'undefined') {%>
			<html>
				<form action="/viewcertification" method="GET" id="viewcertification">
					<input type="hidden" name="certificationid" value="<%=certificationid%>"/>
					<input type="hidden" name="action" value="view"/>
				</form>
				<script type="text/javascript">
					function submitForm(){
						var viewcertification=document.getElementById('viewcertification');
						//alert('<%=certificationid%> -->'+viewcertification);
						viewcertification.submit();
					}
					submitForm();
				</script>
			</html>
		<% } %>
	<% } %>
	
<% } %>


<% if(from == '') {%>
		<% if(typeof userid != 'undefined') {%>
			<%= userid%>
		<% } %>
	<% } %>