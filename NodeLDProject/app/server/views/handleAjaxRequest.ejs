<% if(typeof from != 'undefined') {%>

	<% if(from == 'validateprojectrolename') {%>
		<% if(typeof projectrolename != 'undefined') {%>
			<%= projectrolename%>
		<% } %>
	<% } %>
	
	<% if(from == 'validateuserTagging' || from == 'validateuserid') {%>
		<% if(typeof userid != 'undefined') {%>
			<%= userid%>
		<% } %>
	<% } %>
	<% if(from == 'getUsersByProjectId') {%>
		<% if(typeof users != 'undefined') {%>
			<%= users%>
		<% } %>
	<% } %>
	
	<% if(from == 'adduser') {%>
		<% if(typeof userid != 'undefined') {%>
			<html>
				<form action="/viewuser" method="GET" id="viewuserform">
					<input type="hidden" name="userid" value="<%=userid%>"/>
				</form>
				<script type="text/javascript">
					function submitForm(){
						var viewuserform=document.getElementById('viewuserform');
						//alert('<%=userid%> -->'+viewuserform);
						viewuserform.submit();
					}
					submitForm();
				</script>
			</html>
		<% } %>
	<% } %>
	
	<% if(from == 'router') {%>
		<html>
			<head>
				<script type="text/javascript">
					function mytimeout(){
						setTimeout(setTimeout, 2000);
					}
					function setTimeout(){
						window.top.location.href='/login';
					}
					mytimeout();
				</script>
			</head>
			<body>
				<b>Redirecting to home page as session not exit ...</b>
			</body>
		</html>	
	<% } %>

	<% if(from == 'addproject') {%>
		<% if(typeof projid != 'undefined') {%>
			<html>
				<form action="/viewproject" method="GET" id="viewprojform">
