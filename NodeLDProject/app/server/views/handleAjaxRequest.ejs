				</form>
				<script type="text/javascript">
					function submitForm(){
						var viewtrainingform=document.getElementById('viewtrainingform');
						//alert('<%=trainingid%> -->'+viewtrainingform);
						viewtrainingform.submit();
					}
					submitForm();
				</script>
			</html>
		<% } %>
	<% } %>
	
	<% if(from == 'addprojectrole') {%>
		<% if(typeof projroleid != 'undefined') {%>
			<html>
				<form action="/viewprojectrole" method="GET" id="viewprojroleform">
					<input type="hidden" name="projroleid" value="<%=projroleid%>"/>
				</form>
				<script type="text/javascript">
